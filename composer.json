{
    "name": "fontech/nova-searchable-select-filter",
    "description": "Searchable select filter for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "searchable",
        "filter"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Fontech\\SearchableSelectFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fontech\\SearchableSelectFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
